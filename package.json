{
  "name": "serverless-event-management-api",
  "version": "1.0.0",
  "description": "nothing",
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development NODE_OPTIONS=--dns-result-order=ipv4first sls offline start --stage offline",
    "dev": "run-p dev:*",
    "deploy:db": "dotenv -e .env.production -c -- tsx scripts/deployDbProd.ts",
    "deploy:app": "cross-env NODE_ENV=production sls deploy --stage prod",
    "deploy-prod": "run-s deploy:*",
    "remove-prod": "cross-env NODE_ENV=production sls remove --stage prod",
    "clean": "rimraf dist .esbuild .serverless",
    "lint": "eslint .",
    "format": "eslint --fix . && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "check-types": "tsc --noEmit --pretty",
    "db:push": "prisma db push",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cookie-session": "^2.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "i": "^0.3.7",
    "jsonwebtoken": "^9.0.2",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.4.0",
    "nodemailer": "^6.9.8",
    "npm": "^10.2.5",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-session": "^2.0.48",
    "@types/cross-spawn": "^6.0.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "esbuild": "~0.16.17",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "p-retry": "^4.6.2",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "rimraf": "^5.0.5",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.49.0",
    "serverless-offline": "^13.3.0",
    "start-server-and-test": "^2.0.3",
    "supertest": "^6.3.3",
